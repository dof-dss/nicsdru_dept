    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "github",
            "url": "git@github.com:dof-dss/nicsdru_origins_modules.git",
            "no-api": true
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/highcharts",
                "version": "10.0.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://code.highcharts.com/zips/Highcharts-10.0.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "deaemth/easychart",
